kind: Config
clusters:
- cluster:
    certificate-authority: credentials/ca.pem
    server: https://k8s.kubeops.net
  name: kube-aws-k8s-cluster
contexts:
- context:
    cluster: kube-aws-k8s-cluster
    namespace: default
    user: kube-aws-k8s-admin
  name: kube-aws-k8s-context
users:
- name: kube-aws-k8s-admin
  user:
    client-certificate: credentials/admin.pem
    client-key: credentials/admin-key.pem
current-context: kube-aws-k8s-context
